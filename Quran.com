::-webkit-scrollbar { inline-size: 10px; block-size: 10px; }

::-webkit-scrollbar-track { background-color: transparent; }

::-webkit-scrollbar-thumb { background-color: var(--scrollbar-thumb); border-radius: 5px; }

::-webkit-scrollbar-thumb:hover { background-color: var(--scrollbar-thumb-hover); }

::-webkit-scrollbar-thumb:active { background-color: var(--scrollbar-thumb-active); }

textarea { overflow: auto; vertical-align: top; }

table { border-collapse: collapse; border-spacing: 0px; }

::selection { background: rgb(179, 212, 252); text-shadow: none; }

button, html, input, select, textarea { color: rgb(34, 34, 34); }

img { vertical-align: middle; }

textarea { resize: vertical; }

.chromeframe { margin-block: 0.2em; margin-inline: 0px; background: rgb(204, 204, 204); color: rgb(0, 0, 0); padding-block: 0.2em; padding-inline: 0px; }

